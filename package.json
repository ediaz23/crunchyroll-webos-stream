{
    "name": "com.crunchyroll.webos.stream",
    "version": "1.1.8",
    "description": "Unofficial Crunchyroll app for LG Smart TVs.",
    "main": "src/index.js",
    "scripts": {
        "serve": "NODE_ENV=development REACT_APP_SERVING=true node ./scripts/serve.js",
        "serve-p": "NODE_ENV=production REACT_APP_SERVING=true node ./scripts/serve.js",
        "pack": "NODE_ENV=development REACT_APP_SERVING=false node ./scripts/pack.js",
        "pack-p": "NODE_ENV=production REACT_APP_SERVING=false node ./scripts/pack.js -p",
        "watch": "NODE_ENV=development REACT_APP_SERVING=false node ./scripts/pack.js --watch",
        "clean": "node ./scripts/clean.js",
        "lint": "node ./scripts/lint.js .",
        "license": "node ./scripts/license.js",
        "test": "node ./scripts/test.js",
        "test-watch": "node ./scripts/test.js --watch",
        "build": "NODE_ENV=development gulp build",
        "build-dev": "NODE_ENV=development gulp build-dev",
        "build-p": "NODE_ENV=production gulp build-p",
        "build-service": "gulp build-service",
        "install-tv": "gulp install-app",
        "install-tv-1": "gulp install-app-tv1",
        "start": "ares-launch com.crunchyroll.stream.app",
        "start-1": "ares-launch -d tv1 com.crunchyroll.stream.app",
        "stats": "NODE_ENV=development REACT_APP_SERVING=false node ./scripts/pack.js --stats",
        "stats-p": "NODE_ENV=production REACT_APP_SERVING=false node ./scripts/pack.js -p --stats",
        "manifest": "gulp manifest",
        "play": "NODE_ENV=development npm run build-dev && npm run install-tv && npm run start",
        "play-1": "NODE_ENV=development npm run build-dev && npm run install-tv-1 && npm run start-1",
        "play-p": "NODE_ENV=production npm run build-p && npm run install-tv-1 && npm run start-1",
        "translate": "npx i18next-scanner --config ./i18next-scanner.config.react.js",
        "postinstall": "gulp clean-ilib"
    },
    "author": "Esteban DÃ­az <esteban.diazt23@gmail.com>",
    "license": "Apache-2.0",
    "private": true,
    "repository": "https://github.com/ediaz23/crunchyroll-webos-stream",
    "keywords": [
        "anime",
        "crunchyroll",
        "webos",
        "player",
        "react",
        "enact",
        "recoil",
        "moonstone"
    ],
    "enact": {
        "theme": "moonstone",
        "resolveFallback": {
            "fs": false,
            "path": false,
            "node-fetch": false,
            "form-data": false
        },
        "packagerOptions": {
            "ignore": [
                "service/**",
                "org.webosbrew.manifest.json",
                "screenshots"
            ]
        }
    },
    "engines": {
        "node": ">=18.20.2"
    },
    "eslintConfig": {
        "extends": "enact"
    },
    "eslintIgnore": [
        "node_modules/*",
        "build/*",
        "dist/*",
        "service/*",
        "libs/*",
        "config/*",
        "scripts/*"
    ],
    "dependencies": {
        "@cospired/i18n-iso-languages": "^4.2.0",
        "@enact/core": "~4.8.0",
        "@enact/i18n": "~4.8.0",
        "@enact/moonstone": "~4.5.6",
        "@enact/spotlight": "~4.8.0",
        "@enact/ui": "~4.8.0",
        "@enact/webos": "~4.8.0",
        "classnames": "^2.5.1",
        "core-js": "3.23.3",
        "crunchyroll-js-api": "^1.3.1",
        "dashjs": "^4.7.4",
        "element-closest-polyfill": "^1.0.7",
        "fake-xml-http-request": "^2.1.2",
        "html-entities": "^2.5.2",
        "i18n-iso-countries": "^7.13.0",
        "i18n-iso-m49": "^1.0.0",
        "ilib": "^14.20.0",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "recoil": "^0.7.7",
        "regenerator-runtime": "^0.14.1",
        "uuid": "^10.0.0",
        "web-vitals": "^4.0.0",
        "webostvjs": "^1.2.5",
        "whatwg-fetch": "^3.6.20"
    },
    "resolutions": {
        "//": "See https://github.com/facebook/create-react-app/issues/11773",
        "react-error-overlay": "6.0.9"
    },
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.24.8",
        "@babel/plugin-transform-runtime": "^7.25.4",
        "@enact/dev-utils": "^6.1.0",
        "@enact/template-sandstone": "^2.8.0",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
        "@testing-library/jest-dom": "^6.2.0",
        "@testing-library/react": "^14.1.2",
        "@testing-library/user-event": "^14.5.2",
        "@types/core-js": "^2.5.8",
        "@types/react": "^18.3.12",
        "@types/webostvjs": "^1.2.5",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.3",
        "babel-plugin-dynamic-import-node": "^2.3.3",
        "babel-preset-enact": "^0.1.7",
        "case-sensitive-paths-webpack-plugin": "^2.4.0",
        "chalk": "^5.3.0",
        "cross-spawn": "^7.0.3",
        "css-loader": "^6.8.1",
        "css-minimizer-webpack-plugin": "^5.0.1",
        "del": "^8.0.0",
        "dotenv": "^16.3.1",
        "dotenv-expand": "^10.0.0",
        "eslint": "^8.56.0",
        "eslint-config-enact": "^4.2.1",
        "eslint-config-enact-proxy": "^1.0.8",
        "eslint-webpack-plugin": "^4.0.1",
        "expose-loader": "^4.1.0",
        "file-loader": "^6.2.0",
        "filesize": "^10.1.0",
        "fs-extra": "^11.2.0",
        "gulp": "^5.0.0",
        "gulp-shell": "^0.8.0",
        "html-webpack-plugin": "^5.6.0",
        "i18next-scanner": "^4.6.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-watch-typeahead": "^2.2.2",
        "less": "^4.2.0",
        "less-loader": "^11.1.4",
        "license-checker": "^25.0.1",
        "mini-css-extract-plugin": "^2.7.6",
        "minimist": "^1.2.8",
        "node-polyfill-webpack-plugin": "^3.0.0",
        "postcss": "^8.4.32",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-global-import": "^1.0.6",
        "postcss-loader": "^7.3.4",
        "postcss-normalize": "^10.0.1",
        "postcss-preset-env": "^9.3.0",
        "postcss-resolution-independence": "^1.1.5",
        "prompts": "^2.4.2",
        "react-dev-utils": "^12.0.1",
        "react-error-overlay": "^6.0.11",
        "react-refresh": "^0.14.0",
        "react-test-renderer": "^18.2.0",
        "resolution-independence": "^1.0.0",
        "resolve": "^1.22.8",
        "sass-loader": "^13.3.3",
        "source-map-loader": "^4.0.2",
        "strip-ansi": "^7.1.0",
        "style-loader": "^3.3.3",
        "terser-webpack-plugin": "^5.3.10",
        "webpack": "^5.89.0",
        "webpack-dev-server": "^4.15.1"
    },
    "browserslist": [
        "Chrome 38",
        ">1%",
        "last 2 versions",
        "not ie < 12",
        "not ie_mob < 12"
    ]
}
