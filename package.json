{
    "name": "com.crunchyroll.webos.stream",
    "version": "2.1.2",
    "description": "Unofficial Crunchyroll app for LG Smart TVs.",
    "main": "src/index.js",
    "scripts": {
        "serve": "NODE_ENV=development REACT_APP_SERVING=true GENERATE_SOURCEMAP=false node ./scripts/serve.js",
        "serve-p": "NODE_ENV=production REACT_APP_SERVING=true node ./scripts/serve.js",
        "pack": "NODE_ENV=development REACT_APP_SERVING=false node ./scripts/pack.js",
        "pack-p": "NODE_ENV=production REACT_APP_SERVING=false node ./scripts/pack.js -p",
        "watch": "gulp watch",
        "watch-raw": "NODE_ENV=development REACT_APP_SERVING=false GENERATE_SOURCEMAP=false node ./scripts/pack.js --watch",
        "clean": "node ./scripts/clean.js",
        "lint": "node ./scripts/lint.js -l .",
        "license": "node ./scripts/license.js",
        "test": "node ./scripts/test.js",
        "test-watch": "node ./scripts/test.js --watch",
        "build": "NODE_ENV=development gulp build",
        "build-dev": "NODE_ENV=development gulp build-dev",
        "build-p": "NODE_ENV=production gulp build-p",
        "build-service": "gulp build-service",
        "install-tv": "gulp install-app",
        "install-tv-1": "gulp install-app-tv1",
        "start": "ares-launch com.crunchyroll.stream.app",
        "start-1": "ares-launch -d tv1 com.crunchyroll.stream.app",
        "stats": "NODE_ENV=development REACT_APP_SERVING=false node ./scripts/pack.js --stats",
        "stats-p": "NODE_ENV=production REACT_APP_SERVING=false node ./scripts/pack.js -p --stats",
        "manifest": "gulp manifest",
        "play": "NODE_ENV=development npm run build-dev && npm run install-tv && npm run start",
        "play-1": "NODE_ENV=development npm run build-dev && npm run install-tv-1 && npm run start-1",
        "play-p": "NODE_ENV=production npm run build-p && npm run install-tv-1 && npm run start-1",
        "translate": "npx i18next-scanner --config ./i18next-scanner.config.react.js",
        "postinstall": "gulp gulp-postinstall"
    },
    "author": "Esteban DÃ­az <esteban.diazt23@gmail.com>",
    "license": "Apache-2.0",
    "private": true,
    "repository": "https://github.com/ediaz23/crunchyroll-webos-stream",
    "keywords": [
        "anime",
        "crunchyroll",
        "webos",
        "player",
        "react",
        "enact",
        "recoil",
        "moonstone"
    ],
    "enact": {
        "theme": "moonstone",
        "resolveFallback": {
            "fs": false,
            "path": false,
            "node-fetch": false,
            "form-data": false
        },
        "packagerOptions": {
            "ignore": [
                "service/**",
                "org.webosbrew.manifest.json",
                "screenshots",
                "docker"
            ]
        }
    },
    "engines": {
        "node": ">=20"
    },
